import org.twdata.enchanter.*;

ssh = new GanymedSSH();
ssh.debug = true;
ssh.connect("www.twdata.org", "mrdon");
ssh.setTimeout(10000);
ssh.respond(":~>", "ls -l\n");
ssh.waitFor("incoming\r\n");
print("\n"+"next line:"+ssh.getLine()+":");
ssh.respond(":~>", null);

id = ssh.waitForMux(new String[]{":~>","bar"});
print("id:"+id);
ssh.disconnect();

